{
  "extends": "airbnb",
  "rules": {
    "no-console": "off",
    "arrow-parens": "off",
    "no-plusplus": "off",
    "prefer-const": "off",
    "no-param-reassign": "off",
    "comma-dangle": "off",
    "no-underscore-dangle": "off",
    "function-paren-newline": "off",
    "react/prefer-stateless-function": "off",
    "react/jsx-filename-extension": "off"
  }
}
